{{each feeds}}
	<input type="radio" name="${type}-preset-sources" id="source-${$value._id}" data-address="${$value.address}" class="preset-source" value="${$value._id}" data-role="none">
	<label for="source-${name}-${$value._id}" class="clearfix">
		${$value.title} 
		<a 
			href="" 
			data-role="button" 
			class="add-preset-source ui-btn ui-btn-inline ui-shadow ui-btn-corner-all ui-btn-icon-notext ui-btn-up-b right" 
			data-iconpos="notext" 
			data-icon="plus" 
			data-inline="true">
			
			<span class="ui-btn-inner ui-btn-corner-all">
				<span class="ui-btn-text">Add Preset Source</span>
				<span class="ui-icon ui-icon-plus ui-icon-shadow">&nbsp;</span>
			</span>
		</a>
	</label>
{{/each}}